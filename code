#include<stdio.h> 
int main()
{
int num,sum1,sum2,temp,position;
scanf("%d",&num);
while(num!=0)
{
    temp=num%10;
    if(position%2==0)
    {
       sum1=sum1+temp; 
    }
    else
    sum2=sum2+temp;
    num=num/10;
    position++;
}
printf("Odd Position Sum = %d\n",sum2);
printf("Even Position Sum = %d\n",sum1);
    return 0; 
}
